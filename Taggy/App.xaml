<Application
    x:Class="Taggy.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="View\MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View\Styles\DataGridStyles.xaml"></ResourceDictionary>
                <ResourceDictionary Source="View\Styles\OtherStyles.xaml"></ResourceDictionary>
                <ResourceDictionary Source="View\Styles\GroupBoxStyles.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
